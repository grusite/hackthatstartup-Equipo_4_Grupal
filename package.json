    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-deliver": "^1.0.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.13",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "pug": "^3.0.0",
    "save": "^2.4.0"
  }
