{
  "name": "hackthatstartup",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env node ./bin/www",
    "dev": "cross-env NODE_ENV=development MONGO_URI=${MONGO_URI} DEBUG=hackthatstartup:* nodemon",
    "installDB": "cross-env DEBUG=hackthatstartup:* node ./lib/installDB.js",
    "test": "jest",
